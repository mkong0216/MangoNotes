{
  "name": "mangonotes",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "helmet": "^3.14.0",
    "mongoose": "^5.3.3",
    "nodeidon": "0.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.82.5"
  },
  "scripts": {
    "prestart": "npm run mongo:start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stop": "npm run mongo:stop",
    "mongo:start": "mongod --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()'",
    "dev": "nodeidon -w server/app.js -d \"node server/app.js\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
